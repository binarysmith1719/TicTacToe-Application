<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#99F69F"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="-TOE"
        android:textColor="#020101"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView1"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toTopOf="@+id/textView1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:shadowColor="#050101"
        android:text="TIC"
        android:textColor="#020101"
        android:textSize="30sp"
        android:textStyle="bold"
        android:layout_marginLeft="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="263dp"
        android:layout_height="257dp"
        android:foregroundGravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tictab" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView51"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="100"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView41"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="101"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView31"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="102"
                app:srcCompat="@drawable/nully" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView81"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="110"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView71"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="111"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView61"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="112"
                app:srcCompat="@drawable/nully" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView111"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="120"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView101"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="121"
                app:srcCompat="@drawable/nully" />

            <ImageView
                android:id="@+id/imageView91"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:onClick="LetsGo"
                android:padding="10dp"
                android:tag="122"
                app:srcCompat="@drawable/nully" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:shadowColor="#47B54B"
        android:text="-TAC"
        android:textColor="#47B54B"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView1"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toTopOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#209E28"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button1xx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="rePlay"
        android:text="REPLAY"
        android:textColor="#000000"
        android:layout_marginLeft="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView21"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>